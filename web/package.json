{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/vanity-web",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://vanity.polymedia.app",
    "description": "Generate Sui vanity addresses in your browser",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-vanity"
    },
    "keywords": [],
    "scripts": {
        "build": "vite build",
        "clean": "rm -rf dist/ node_modules/ .wrangler/",
        "deploy-dev": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-vanity --branch dev",
        "deploy-prod": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-vanity --branch main",
        "dev": "vite",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "pnpm build && vite preview",
        "tunnel": "lt --port 1234 --subdomain poly"
    },
    "dependencies": {
        "@mysten/sui.js": "^0.51.0",
        "@scure/bip39": "^1.3.0",
        "normalize.css": "^8.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.3"
    },
    "devDependencies": {
        "@types/react": "^18.2.72",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "@vitejs/plugin-react-swc": "^3.6.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "less": "^4.2.0",
        "localtunnel": "^2.0.2",
        "typescript": "^5.4.3",
        "vite": "^5.2.6"
    },
    "type": "module",
    "sideEffects": false
}
